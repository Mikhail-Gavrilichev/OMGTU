using System;

class H
{
    static void Main()
    {
        int all = 0;
        int N = Convert.ToInt32(Console.ReadLine());

        for (int i = 0; i < N; i++)
        {
            int a = Convert.ToInt32(Console.ReadLine());
            int t = a % 3;
            int p = a % 5;
            int c = a % 7;

            if ((t==a)&(a==c)&(a==p))
            {
                all++;
            }
        }
        Console.WriteLine(all);
    }
}
