//Для 10 вводимых элементов найти сумму чисел кратных трём.
using System;
class HelloWorld {
static void Main() {
int b=0;
int sym=0;
for (int i=0; i<=9; i++)
   {
        int n=Convert.ToInt32(Console.ReadLine());
        b=n;
        
      if (b<0)
      
      {
          b=-b;
      }
        
   while (b>=3)
   
   { 
       b=b-3;
   }

     if (b==0)
    
     {
        
     sym=sym+n;
  
     }  
   }
   Console.WriteLine(sym);
}
}
